{
  "name": "template-koa-ts",
  "private": true,
  "version": "1.0.0",
  "description": "webpack typescript koa app",
  "main": "app.ts",
  "scripts": {
    "webpack:debug": "node --inspect-brk ./node_modules/.bin/webpack --inline --progress",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --config config/webpack.config.dev.ts",
    "debug": "nodemon ./dist/server.debug.js 9999 -- --inspect",
    "dev": "npm-run-all -p watch debug",
    "build": "cross-env NODE_ENV=prod webpack --config config/webpack.config.prod.ts",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint": "npm-run-all -p lint:eslint lint:prettier",
    "lint-fix": "npm-run-all -p lint-fix:eslint lint-fix:prettier",
    "lint:eslint": "eslint \"src/**/*.{ts,js}\"",
    "lint-fix:eslint": "eslint \"src/**/*.{ts,js}\" --quiet --fix",
    "lint:prettier": "prettier -c \"src/**/*.{ts,js,json,md}\"",
    "lint-fix:prettier": "prettier --write \"src/**/*.{ts,js,json,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{ts,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/bluebird": "^3.5.38",
    "@types/ip": "^1.1.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "^3.3.1",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-json": "^2.0.20",
    "@types/koa-range": "^0.3.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/nodemailer": "^6.4.7",
    "@types/webpack-node-externals": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-loader": "^9.1.2",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "fs": "^0.0.1-security",
    "husky": "^8.0.3",
    "install": "^0.13.0",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.4",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "bluebird": "^3.7.2",
    "ip": "^1.1.8",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "koa-combine-routers": "^4.0.2",
    "koa-compose": "^4.1.0",
    "koa-compress": "^5.1.0",
    "koa-helmet": "^6.1.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.4",
    "koa-range": "^0.3.0",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "log4js": "^6.8.0",
    "nodemailer": "^6.9.1",
    "redis": "^4.6.5"
  }
}
